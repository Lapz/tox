
class Tree {
  key:string;
  left: Tree;
  right:Tree;
}


fn prettyprint(tree:Tree) -> string {
  let output = "";

  let write =  |s:string| {
    output += s
  };

  let show = |n:i32, t: Tree| {
    let indent = |s:string| {
      for(let i = 0; i < n; i += 1) {
        write(" ");
      }

      output += s;

      write("\n");
    };


    if t == nil {
      indent(".")
    } else {
      indent(t.key);
      show(n+1,t.left);
      show(n+1,t.right);
    }
  };

  show(0,tree);

  return output
}

