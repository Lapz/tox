class Toggle {
    state:bool;
  
    fun value() -> bool {
      return this.state;
    }
  
    fun activate() -> Toggle {
      this.state = !this.state;
      return this;
    }
}

class Button < Toggle {
    state:bool;
  
    fun value() -> bool {
      return this.state;
    }
  
    fun activate() -> Toggle {
      this.state = !this.state;
      return this;
    }
}

